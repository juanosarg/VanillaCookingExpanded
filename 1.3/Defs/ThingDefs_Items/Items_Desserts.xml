<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="ResourceBase">
		<defName>VCE_SimpleDessert</defName>
		<label>simple dessert</label>
		<description>A simple dessert quickly made from a few fruits and other ingredients.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_DessertSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		 <stackLimit>10</stackLimit>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Beauty>0</Beauty>
			<MarketValue>15</MarketValue>
			<WorkToMake>300</WorkToMake>
			<Nutrition>0.2</Nutrition>
			<Mass>0.44</Mass>
			<DeteriorationRate>10</DeteriorationRate>
		</statBases>
		<tickerType>Rare</tickerType>
		<thingCategories>
			<li>FoodMeals</li>
		</thingCategories>
		<tradeTags Inherit="False">
			<li>VCE_JunkFood</li>
		</tradeTags>
		<ingestible>
			<chairSearchRadius>9</chairSearchRadius>
			<preferability>DesperateOnly</preferability>
			<foodType>Processed</foodType>
			<joy>0.3</joy>
			<drugCategory>Social</drugCategory>
			<tasteThought>VCE_AteSimpleDessert</tasteThought>
			<joyKind>VCE_Confectionery</joyKind>
			<maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
			<ingestSound>Meal_Eat</ingestSound>
			<ingestEffect>EatVegetarian</ingestEffect>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>4</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
			<li Class="CompProperties_Forbiddable" />
			<li Class="CompProperties_Ingredients" />
			<li Class="CompProperties_FoodPoisonable" />
			<li Class="CompProperties_Drug">
				<listOrder>1000</listOrder>
				<overdoseSeverityOffset>0.08~0.14</overdoseSeverityOffset>
			</li>
		</comps>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				
				<statToAdd>
					
					<li>VCE_HealthEffects</li>			
				</statToAdd>
				<statValues>
					
					<li>VCE_DiabetesEffect</li>				
				</statValues>
				<statDescriptions>
					
					<li>VCE_DiabetesDessertSimpleDesc</li>
				</statDescriptions>
				
			</li>
		</modExtensions>
	</ThingDef>

