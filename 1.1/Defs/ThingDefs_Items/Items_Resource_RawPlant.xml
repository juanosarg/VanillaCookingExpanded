<Defs>  


	<ThingDef ParentName="PlantFoodRawBase">
		<defName>VCE_RawOnion</defName>
		<label>onions</label>
		<description>An edible bulb of pungent, savory taste and smell with almost as many varieties as cooking applications.</description>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>	

		<graphicData>

			<texPath>Things/Items/Raw/VCE_Onions</texPath>

