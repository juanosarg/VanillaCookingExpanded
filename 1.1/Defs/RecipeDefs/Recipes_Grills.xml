<Defs>


	<RecipeDef Name="VCE_CookGrillMealBase" Abstract="True">
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>PlantFoodRaw</li>
			</categories>
			
			
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>PlantFoodRaw</li>
			</categories>
			
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
	</RecipeDef>

