<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--=========================== Food crops ==============================-->

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Onion</defName>
		<label>onion</label>
		<description>An edible bulb of pungent, savory taste and smell with almost as many varieties as cooking applications. Cannot suffer the effects of blight.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Onion</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.70</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>4</maxMeshCount>
			<harvestedThingDef>VCE_RawOnion</harvestedThingDef>
			<harvestYield>11</harvestYield>
			<harvestWork>300</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.6</growDays>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>0.4~0.8</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Pumpkin</defName>
		<label>pumpkin</label>
		<description>Technically a giant berry, pumpkins are renowned for being trivial to both sow and harvest. However, their durability means added weight and mass - Too cumbersome for hydroponics basins.</description>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_PumpkinPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>30</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.70</fertilityMin>
			<fertilitySensitivity>0.5</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawPumpkin</harvestedThingDef>
			<harvestYield>19</harvestYield>
			<harvestWork>300</harvestWork>
			<sowTags>
				<li>Ground</li>

			</sowTags>
			<harvestFailable>false</harvestFailable>
			<topWindExposure>0.05</topWindExposure>
			<growDays>10.4</growDays>
			<immatureGraphicPath>Things/Plant/Crops/VCE_PumpkinPlantImmature</immatureGraphicPath>
			<visualSizeRange>0.9~0.9</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Pepper</defName>
		<label>pepper</label>
		<description>Tiny, spicy, crimson pods often used in sauces, relishes, or powders. While the flavor itself is mild the capsaicin contained inside means humans find them oddly addicting, if painful when consumed in mass. Animals are known to avoid eating them entirely.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_PepperPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.70</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawPepper</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.2</growDays>
			<immatureGraphicPath>Things/Plant/Crops/VCE_PepperPlantImmature</immatureGraphicPath>
			<visualSizeRange>0.3~1</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Peas</defName>
		<label>peas</label>
		<thingClass>VanillaPlantsExpanded.Plant_RainAffected</thingClass>
		<description>Spherical green seeds tucked safely away in a fleshy pod. Pea plants are known to be quite thirsty, and will benefit greatly from rainfall. Peas are also notable for having potentially long shelf-lives when dried properly.</description>
		<statBases>
			<MaxHitPoints>70</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_PeaPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.70</fertilityMin>
			<fertilitySensitivity>0.85</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawPeas</harvestedThingDef>
			<harvestYield>12</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>6.5</growDays>
			<immatureGraphicPath>Things/Plant/Crops/VCE_PeaPlantImmature</immatureGraphicPath>
			<visualSizeRange>0.3~1</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Eggplant</defName>
		<label>eggplant</label>
		<thingClass>VanillaPlantsExpanded.Plant_HighTempAffected</thingClass>
		<description>A bulbous purple fruit of the nightshade family. Eggplant is rarely served as a side, generally filling the role of the main component of the dish as a meat substitute. Flourishes in warm weather.</description>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Nutrition>0.3</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_EggplantPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>15</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.71</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawEggplant</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>

			<topWindExposure>0.1</topWindExposure>
			<growDays>6</growDays>
			<immatureGraphicPath>Things/Plant/Crops/VCE_EggplantPlantImmature</immatureGraphicPath>
			<visualSizeRange>0.3~1</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Cabbage</defName>
		<label>cabbage</label>
		<thingClass>VanillaPlantsExpanded.Plant_StopsGrowingInToxins</thingClass>
		<description>A somewhat hardy, leafy green vegetable, often chopped and then fermented or stewed. This particular strain seems highly resistant to airborne toxin and radiation.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.3</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Cabbage</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>20</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.7</fertilityMin>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawCabbage</harvestedThingDef>
			<harvestYield>11</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<dieFromToxicFallout>false</dieFromToxicFallout>
			<topWindExposure>0.1</topWindExposure>
			<growDays>6.2</growDays>
			<immatureGraphicPath>Things/Plant/Crops/VCE_CabbageImmature</immatureGraphicPath>
			<visualSizeRange>0.3~1</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Beet</defName>
		<label>beet</label>
		<thingClass>VanillaPlantsExpanded.Plant_WinterResistant</thingClass>
		<description>An herb-like vegetable widely cultivated for humans and livestock alike, notably nutritious and historically used in dye making. While beets do require rich soil they are quite hearty, and will continue to grow very slowly through winter. They need to be planted during growing season, though.</description>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<Nutrition>0.18</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Beet</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>1.1</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>false</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>4</maxMeshCount>
			<harvestedThingDef>VCE_RawBeets</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<sowMinSkill>8</sowMinSkill>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.7</growDays>			
			<visualSizeRange>0.3~0.8</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>VCE_Tomato</defName>
		<label>tomato</label>
		<description>A series of juicy, glossy red fruits attached by vine, used commonly as a vegetable. Strong stewing releases natural sugars and mellows out acids contained within. Tomatoes are easily removed from the vine without having to uproot the plant.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_TomatoPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>20</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>1.1</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_RawTomatoes</harvestedThingDef>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestYield>10</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.7</growDays>			
			<visualSizeRange>0.3~1</visualSizeRange>
			<immatureGraphicPath>Things/Plant/Crops/VCE_TomatoPlantImmature</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>
	
	<ThingDef ParentName="PlantBase">
		<defName>VCE_Wheat</defName>
		<label>wheat</label>
		<description>An amber cereal plant that takes almost an entire growing season to come to fruition. While fragile and time-consuming, this wheat has been genetically engineered to gentle break into flour with a mere touch.</description>
		<statBases>
			<MaxHitPoints>30</MaxHitPoints>
			<Nutrition>0.18</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Wheat</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>40</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>0.7</fertilityMin>
			<fertilitySensitivity>1</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>VCE_Flour</harvestedThingDef>
			<sowMinSkill>6</sowMinSkill>
			<harvestYield>24</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<topWindExposure>0.2</topWindExposure>
			<growDays>13</growDays>			
			<visualSizeRange>0.3~1.2</visualSizeRange>
			
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>



</Defs>
