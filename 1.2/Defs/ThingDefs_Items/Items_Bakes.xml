<?xml version="1.0" encoding="utf-8" ?>
<Defs>





	<ThingDef ParentName="MealCooked">
		<defName>VCE_SimpleBake</defName>
		<label>simple bake</label>
		<description>A simple bake quickly cooked from a few ingredients and flour.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_BakeSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>15</MarketValue>
			<WorkToMake>450</WorkToMake>
			<Nutrition>0.7</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
		<defName>VCE_FineBake</defName>
		<label>fine bake</label>
		<description>A complex bake assembled with care from a wide selection of ingredients and flour.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_BakeFine</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>20</MarketValue>
			<WorkToMake>675</WorkToMake>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealFine</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<tasteThought>AteFineMeal</tasteThought>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
		<defName>VCE_LavishBake</defName>
		<label>lavish bake</label>
		<description>A masterpiece of the culinary arts, this bake nourishes the body, mind, and soul.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_BakeLavish</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>40</MarketValue>
			<WorkToMake>1200</WorkToMake>
			<Nutrition>0.9</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealLavish</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<tasteThought>AteLavishMeal</tasteThought>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>

	</ThingDef>

	<ThingDef ParentName="MealCooked">
		<defName>VCE_GourmetBake</defName>
		<label>gourmet bake</label>
		<description>A culinary work of art, characterized by refined, even elaborate preparation and presentation of an aesthetically balanced, albeit low quantity bake.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_BakeGourmet</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>75</MarketValue>
			<WorkToMake>2250</WorkToMake>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealLavish</preferability>
			<optimalityOffsetHumanlikes>1</optimalityOffsetHumanlikes>
			<tasteThought>VCE_AteGourmetMeal</tasteThought>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<comps>
			<li Class="VanillaCookingExpanded.CompProperties_StackByIngredients" />

		</comps>
	</ThingDef>


</Defs>
